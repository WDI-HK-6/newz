// angular-config.js.erb
var app = angular.module('newz', ['ngRoute', 'angularMoment', 'ui.bootstrap', 'angular-images-loaded', 'angularFileUpload']);

app.config(['$routeProvider',function($routeProvider){
  $routeProvider
  .when('/', {templateUrl: "<%= asset_path('home.html') %>", controller: 'HomeCtrl'})
  .when('/close', {templateUrl: "<%= asset_path('close.html')%>", controller: 'CloseCtrl'})
}]);
